		<Style Selector="idc|DocumentTabStripItem">
			<!--<Setter Property="BorderThickness" Value="0" />
		<Setter Property="BorderBrush" Value="Red" />-->
			<Setter Property="Margin" Value="0 0 1 1" />
			<Setter Property="Background" Value="{DynamicResource SystemChromeMediumColor}" />
			<Setter Property="MinHeight" Value="22" />
		</Style>
		<Style Selector="idc|ToolControl">
			<Setter Property="HeaderTemplate">
				<DataTemplate DataType="core:IDockable">
					<TextBlock Text="{Binding Title}" Padding="1"/>
				</DataTemplate>
			</Setter>
		</Style>
		<!--Code completion-->
		<Style Selector="cc|CompletionList">
			<Setter Property="Template">
				<ControlTemplate>
					<!--Background="Gray" BorderThickness="1" BorderBrush="LightGray"-->
					<cc:CompletionListBox Name="PART_ListBox" >
						<cc:CompletionListBox.ItemTemplate>
							<DataTemplate x:DataType="editor:CompletionDataSql">
								<!--VerticalAlignment="Center" Height="18"-->
								<StackPanel Orientation="Horizontal">
									<Image Source="{Binding Image}"
										   Width="16"
										   Height="16"
										   Margin="0,0,2,0" />
									<!--Foreground="#eeeeee"-->
									<!--FontSize="20" FontFamily="Consolas"-->
									<!--Margin="10,0,0,0"-->
									<TextBlock VerticalAlignment="Center" Text="{Binding Content}"
											   FontSize="{StaticResource CompletitionFontSize}" />
								</StackPanel>
							</DataTemplate>
						</cc:CompletionListBox.ItemTemplate>
					</cc:CompletionListBox>
				</ControlTemplate>
			</Setter>
		</Style>
		<!-- CompletionTipContentControl-->
		<Style Selector="cc|CompletionTipContentControl">
			<Setter Property="Template">
				<ControlTemplate>
					<ContentPresenter Name="PART_ContentPresenter"
									  Background="{TemplateBinding Background}"
									  BorderBrush="{TemplateBinding BorderBrush}"
									  BorderThickness="{TemplateBinding BorderThickness}"
									  CornerRadius="{TemplateBinding CornerRadius}"
									  ContentTemplate="{TemplateBinding ContentTemplate}"
									  Content="{TemplateBinding Content}"
									  Padding="{TemplateBinding Padding}"
									  VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"
									  HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}"
									  MaxWidth="940"
									  MaxHeight="500"/>
				</ControlTemplate>
			</Setter>
		</Style>
	</Application.Styles>
</Application>


